program MJProgram
	int noviInt, a[];
{
	void main() int b[], b2[];
	{
		b = new int[2];
		b[0] = 0;
		b[1] = 1;
		b2 = new int[2];
		b2[0] = 1;
		b2[1] = 2;
		
		print(b[0]);
		print(b[1]);
		print(b2[0]);
		print(b2[1]);
		
		a = b;
		b = b2;
		b2 = new int[2];
		b2[0] = 100;
		b2[1] = 200;
		
		print(a[0]);
		print(a[1]);
		print(b[0]);
		print(b[1]);
		print(b2[0]);
		print(b2[1]);
		
	}
}